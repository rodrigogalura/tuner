{
    "name": "rodrigogalura/tuner",
    "minimum-stability": "dev",
    "description": "A Laravel package to build smart APIs once â€” and let clients do the rest.",
    "type": "library",
    "require": {
        "php": "8.4.*"
    },
    "require-dev": {
        "laravel/pint": "^1.21",
        "pestphp/pest": "^3.7",
        "orchestra/testbench": "^10.1",
        "pestphp/pest-plugin-laravel": "^3.1",
        "rector/rector": "dev-main"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "RGalura\\ApiIgniter\\": [
                "src/",
                "src/Traits/",
                "src/Traits/Core/"
            ],
            "RodrigoGalura\\Tuner\\": [
                "src/"
            ]
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Rodrigo Galura",
            "email": "rodrigogalura3rd@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php setup-development.php",
            "@php vendor/bin/testbench create:truth-table 'Projection Intersect'",
            "@php vendor/bin/testbench create:truth-table 'Projection Except'"
        ],
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ],
        "pint": [
            "@php vendor/bin/pint --ansi"
        ],
        "PowerSetUnitTest": [
            "@clear",
            "@php vendor/bin/pest tests/Unit/restructured/PowerSetTest.php --profile --bail"
        ],
        "MatrixUnitTest": [
            "@clear",
            "@php vendor/bin/pest tests/Unit/restructured/MatrixTest.php --profile --bail"
        ],
        "truthTableUnitTest": [
            "@clear",
            "@php vendor/bin/pest tests/Unit/restructured/TruthTableTest.php --profile --bail"
        ],
        "unitTest": [
            "@clear",
            "@php vendor/bin/pest tests/Unit/restructured --profile --bail"
        ],
        "featureTest": [
            "@clear",
            "@php vendor/bin/pest tests/Feature/restructured --profile --bail"
        ],
        "helperTest": [
            "@clear",
            "@php vendor/bin/pest tests/Unit/restructured/Core/HelperTest.php --profile --bail"
        ],
        "projectionFeatureTest": [
            "@clear",
            "@php vendor/bin/pest tests/Feature/restructured/ProjectionTest.php --profile --bail"
        ],
        "projectionUnitTest": [
            "@clear",
            "@php vendor/bin/pest tests/Unit/restructured/ProjectionTest.php --profile --bail"
        ],
        "searchFeatureTest": [
            "@clear",
            "@php vendor/bin/pest tests/Feature/restructured/SearchTest.php --profile --bail"
        ],
        "searchUnitTest": [
            "@clear",
            "@php vendor/bin/pest tests/Unit/restructured/SearchTest.php --profile --bail"
        ],
        "sortFeatureTest": [
            "@clear",
            "@php vendor/bin/pest tests/Feature/restructured/SortTest.php --profile --bail"
        ],
        "sortUnitTest": [
            "@clear",
            "@php vendor/bin/pest tests/Unit/restructured/SortTest.php --profile --bail"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "RodrigoGalura\\Tuner\\TunerServiceProvider"
            ]
        }
    }
}
